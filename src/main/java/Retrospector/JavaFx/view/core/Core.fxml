<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="anchor" prefHeight="800.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="retrospector.javafx.view.core.CoreController">
   <center>
      <TabPane fx:id="tabs" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab fx:id="searchTab" text="Search" />
            <Tab fx:id="mediaTab" text="Media" />
            <Tab fx:id="chartTab" disable="true" text="Chart" />
            <Tab fx:id="wishlistTab" disable="true" text="Wishlist" />
            <Tab fx:id="listTab" disable="true" text="List" />
            <Tab fx:id="achievementTab" disable="true" text="Achievements" />
         </tabs>
      </TabPane>
   </center>
   <top>
      <MenuBar fx:id="menuBar" prefHeight="35.0">
         <menus>
            <Menu disable="true" mnemonicParsing="false" text="File" />
            <Menu disable="true" mnemonicParsing="false" text="Help" />
         </menus>
      </MenuBar>
   </top>
</BorderPane>
